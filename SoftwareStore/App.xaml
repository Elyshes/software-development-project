<Application x:Class="SoftwareStore.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:bookstore="clr-namespace:SoftwareStore">
    <Application.Resources>
      <ResourceDictionary>
        <ResourceDictionary.MergedDictionaries >
          <ResourceDictionary>
            <bookstore:ApplicationBootstrapper x:Key="Bootstrapper" />
          </ResourceDictionary>
        </ResourceDictionary.MergedDictionaries>
      </ResourceDictionary>
    </Application.Resources>
</Application>
